

QMainWindow {
    background-color: #222222
}

QMenuBar {
    background-color: #222222
}


QPushButton {
    background-color: #777777;
    border-radius: 0px;
    color: #000000
}

QPushButton:checked, QPushButton:hover:checked, QPushButton:pressed {
    background-color: #b3d9ff;
    border-style: inset;
    color: #000000
}

QPushButton:hover {
    background-color: #707070
}